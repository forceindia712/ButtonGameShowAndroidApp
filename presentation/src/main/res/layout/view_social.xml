<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/visit_us_on_textview"
        style="@style/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/visit_us_on"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/github_forceindia"
        style="@style/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/github_forceindia"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/visit_us_on_textview"
        app:layout_constraintTop_toBottomOf="@+id/visit_us_on_textview" />

    <TextView
        android:id="@+id/github_dawiczku"
        style="@style/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/github_Dawiczku"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/github_imageView2"
        app:layout_constraintTop_toBottomOf="@+id/visit_us_on_textview" />

    <View
        android:id="@+id/view1"
        android:layout_width="70dp"
        android:layout_height="1dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="2dp"
        android:background="?android:textColorPrimary"
        app:layout_constraintBottom_toBottomOf="@+id/visit_us_on_textview"
        app:layout_constraintEnd_toStartOf="@+id/visit_us_on_textview"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/visit_us_on_textview"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view2"
        android:layout_width="70dp"
        android:layout_height="1dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="2dp"
        android:background="?android:textColorPrimary"
        app:layout_constraintBottom_toBottomOf="@+id/visit_us_on_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/visit_us_on_textview"
        app:layout_constraintTop_toTopOf="@+id/visit_us_on_textview"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/github_imageView1"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/github_forceindia"
        app:layout_constraintEnd_toStartOf="@+id/github_forceindia"
        app:layout_constraintTop_toTopOf="@+id/github_forceindia"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/github_icon" />

    <ImageView
        android:id="@+id/github_imageView2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/github_dawiczku"
        app:layout_constraintStart_toEndOf="@+id/visit_us_on_textview"
        app:layout_constraintTop_toTopOf="@+id/github_dawiczku"
        app:srcCompat="@drawable/github_icon" />

    <View
        android:id="@+id/github_forceindia_button_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/github_forceindia"
        app:layout_constraintEnd_toEndOf="@+id/github_forceindia"
        app:layout_constraintStart_toStartOf="@+id/github_imageView1"
        app:layout_constraintTop_toTopOf="@+id/github_imageView1" />

    <View
        android:id="@+id/github_dawiczku_button_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/github_dawiczku"
        app:layout_constraintEnd_toEndOf="@+id/github_dawiczku"
        app:layout_constraintStart_toStartOf="@+id/github_imageView2"
        app:layout_constraintTop_toTopOf="@+id/github_imageView2" />

</androidx.constraintlayout.widget.ConstraintLayout>